# Demo Mode Environment Variables
# Copy this to .env.local to enable demo mode

# Enable Demo Mode (bypasses authentication and uses mock data)
NEXT_PUBLIC_DEMO_MODE=false

# Demo API URL (not used in demo mode, but required by some libraries)
NEXT_PUBLIC_API_URL=http://localhost:8000

# Optional: Set to true to see debug info in console
NEXT_PUBLIC_DEBUG=true